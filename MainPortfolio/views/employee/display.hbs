<h3>Student Information </h3>
<a class="btn btn-secondary" href="/employee/list"></i>Back</a> 
<table class ="table table-striped">
    <thead>
      <tr>
          <th>Full Name </th>
          <th>Mobile </th>
          <th>Email </th>
          <th> City </th>
          <th></th>
      </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
          <td>{{this.fullName}}</td>
          <td>{{this.mobile}}</td>
          <td>{{this.email}}</td>
          <td>{{this.city}}</td>
          <td>
              <a href="/employee/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
              <a href="/employee/delete/{{this._id}}" onclick="return confirm('Are you really want to delete?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
          </td>
        </tr>
        {{/each}}
    </tbody>
</table>
